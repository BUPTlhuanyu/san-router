{
    "name": "san-router",
    "version": "0.1.0",
    "description": "Simple SPA router",
    "main": "src/bootstrap/main.js",
    "scripts": {
        "test": "edp test start --singleRun",
        "doc": "esdoc -c esdoc.json",
        "compile": "babel -d dist/ src/",
        "prepublish": "npm run compile"
    },
    "edp": {
        "wwwroot": "/",
        "depDir": "dep",
        "srcDir": "src",
        "main": "bootstrap/main.js",
        "loaderAutoConfig": "js,htm,html,tpl,vm,phtml",
        "loaderUrl": "http://s1.bdstatic.com/r/www/cache/ecom/esl/1-8-2/esl.js",
        "dependencies": {
            "mini-event": "2.x"
        }
    },
    "dependencies": {
        "mini-event": "2.x"
    },
    "devDependencies": {
        "babel-cli": "^6.8.0",
        "babel-core": "^6.8.0",
        "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
        "babel-preset-es2015": "^6.6.0",
        "esdoc": "^0.4.4",
        "esdoc-es7-plugin": "0.0.3"
    },
    "author": "otakustay",
    "repository": "https://github.com/ecomfe/san-router",
    "license": "MIT",
    "readme": "README"
}